{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/prop-types": "off",
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/label-has-for": "off",
    "class-methods-use-this": "off",
    "comma-dangle": "off",
    "no-shadow": "off",
    "no-console": "off"
  },
  "env": {
    "browser": true
  }
}
